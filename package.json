{
    "name": "gogit-work",
    "productName": "gogit-work",
    "version": "1.0.1",
    "author": "lizhooh",
    "private": true,
    "description": "gogit 守护进程可视化客户端。",
    "license": "MIT",
    "main": "./dist/electron/main.js",
    "scripts": {
        "build": "node .electron-react/build.js",
        "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-react/build.js",
        "build:linux": "cross-env BUILD_TARGET=linux node .electron-react/build.js",
        "build:mas": "cross-env BUILD_TARGET=mas node .electron-react/build.js",
        "build:win": "cross-env BUILD_TARGET=win32 node .electron-react/build.js",
        "build:clean": "cross-env BUILD_TARGET=clean node .electron-react/build.js",
        "build:web": "cross-env BUILD_TARGET=web node .electron-react/build.js",
        "start": "node .electron-react/dev-runner.js",
        "dev": "node .electron-react/dev-runner.js",
        "pack": "npm run pack:main && npm run pack:renderer",
        "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-react/webpack.main.config.js",
        "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-react/webpack.renderer.config.js",
        "postinstall": ""
    },
    "dependencies": {
        "color": "^2.0.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-router-dom": "^4.2.2",
        "styled-components": "^2.4.0",
        "vue-electron": "^1.0.6"
    },
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.24.1",
        "babili-webpack-plugin": "^0.1.2",
        "cfonts": "^1.1.3",
        "chalk": "^2.1.0",
        "copy-webpack-plugin": "^4.0.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.4",
        "del": "^3.0.0",
        "devtron": "^1.4.0",
        "electron": "^1.7.5",
        "electron-debug": "^1.4.0",
        "electron-devtools-installer": "^2.2.0",
        "electron-packager": "^8.5.0",
        "electron-rebuild": "^1.1.3",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "html-webpack-plugin": "^2.30.1",
        "multispinner": "^0.2.1",
        "node-loader": "^0.6.0",
        "style-loader": "^0.18.2",
        "url-loader": "^0.5.9",
        "webpack": "^3.5.2",
        "webpack-dev-server": "^2.7.1",
        "webpack-hot-middleware": "^2.18.2"
    }
}
